import {
  Injectable,
  NotFoundException,
  ConflictException,
  UnauthorizedException,
} from '@nestjs/common';
import { RegisterDto } from './dto/register.dto';
import { LoginDto } from './dto/login.dto';
import { UpdateParticipantDto } from './dto/update-participant.dto';
import { Participant } from '../../common/interfaces/participant.interface';
import { Role } from '../../common/enums/role.enum';
import { JsonDB } from '../../common/utils/json-db.util';
import { randomUUID } from 'crypto';

@Injectable()
export class ParticipantsService {
  private readonly db: JsonDB<Participant>;

  constructor() {
    this.db = new JsonDB<Participant>('participants');
  }

  register(registerDto: RegisterDto): Omit<Participant, 'password'> {
    const participants = this.db.read();

    // ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤ Username ‡∏´‡∏£‡∏∑‡∏≠ Email ‡∏ã‡πâ‡∏≥‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
    const isDuplicate = participants.some(
      (p) =>
        p.username === registerDto.username || p.email === registerDto.email,
    );
    if (isDuplicate) {
      throw new ConflictException('Username ‡∏´‡∏£‡∏∑‡∏≠ Email ‡∏ô‡∏µ‡πâ‡∏°‡∏µ‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß'); // ‡πÇ‡∏¢‡∏ô 409 Conflict
    }

    const now = new Date();
    const newParticipant: Participant = {
      userId: randomUUID(),
      username: registerDto.username,
      password: registerDto.password, // ‡∏Ç‡∏≠‡∏á‡∏à‡∏£‡∏¥‡∏á‡∏ï‡πâ‡∏≠‡∏á Hash (‡πÄ‡∏ä‡πà‡∏ô bcrypt) ‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏ã‡∏ü‡∏ô‡∏∞‡∏Ñ‡∏£‡∏±‡∏ö
      email: registerDto.email,
      firstName: registerDto.firstName,
      lastName: registerDto.lastName,
      phoneNumber: registerDto.phoneNumber,
      role: Role.PARTICIPANT, // ‡∏Å‡∏≥‡∏´‡∏ô‡∏î Role ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
      isActive: true, // ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÄ‡∏õ‡πá‡∏ô true
      createdAt: now,
    };

    participants.push(newParticipant);
    this.db.write(participants);

    // ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡πÇ‡∏î‡∏¢‡∏ï‡∏±‡∏î‡∏£‡∏´‡∏±‡∏™‡∏ú‡πà‡∏≤‡∏ô‡∏ó‡∏¥‡πâ‡∏á‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
    const { password, ...result } = newParticipant;
    return result;
  }

  login(loginDto: LoginDto): Omit<Participant, 'password'> {
    const participants = this.db.read();

    // ‡∏´‡∏≤ User ‡∏ó‡∏µ‡πà Username ‡πÅ‡∏•‡∏∞ Password ‡∏ï‡∏£‡∏á‡∏Å‡∏±‡∏ô
    const user = participants.find(
      (p) =>
        p.username === loginDto.username && p.password === loginDto.password,
    );

    if (!user) {
